<%- model_class = Banner -%>
<div class="page-header">
  <h1><%=t '.title', :default => Banner %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:title) %></dt>
      <dd><pre class="prettyprint"><%= @banner.title %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:utm_campaign) %></dt>
      <dd><pre class="prettyprint"><%= @banner.utm_campaign %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:utm_source) %></dt>
      <dd><pre class="prettyprint"><%= @banner.utm_source %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:utm_medium) %></dt>
      <dd><pre class="prettyprint"><%= @banner.utm_medium %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:utm_content) %></dt>
      <dd><pre class="prettyprint"><%= @banner.utm_content %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:utm_term) %></dt>
      <dd><pre class="prettyprint"><%= @banner.utm_term %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:md5) %></dt>
      <dd><pre class="prettyprint"><%= @banner.md5 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:campaign_tracker) %></dt>
      <dd><pre class="prettyprint"><%= @banner.campaign_tracker %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:recipients) %></dt>
      <dd><pre class="prettyprint"><%= @banner.recipients %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:reference) %></dt>
      <dd><pre class="prettyprint"><%= @banner.reference %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:additional_ids) %></dt>
      <dd><pre class="prettyprint"><%= @banner.additional_ids %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              banners_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_banner_path(@banner), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              banner_path(@banner),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

